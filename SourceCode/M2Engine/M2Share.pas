      end
      else
        ExpConf.WriteInteger('GuildExp' + IntToStr(i), 'BuildPoint', 0);
    end
    else
      g_Config.GuildLevelExp[i].nBuildPoint := LoadInteger;

    LoadInteger := ExpConf.ReadInteger('GuildExp' + IntToStr(i), 'FlourishingPoint', -1);
