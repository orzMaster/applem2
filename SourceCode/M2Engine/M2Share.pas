      ExpConf.WriteInteger('GuildExp' + IntToStr(i), 'Gold', I * 300000);
    end
    else
      g_Config.GuildLevelExp[i].nGold := LoadInteger;

    LoadInteger := ExpConf.ReadInteger('GuildExp' + IntToStr(i), 'BuildPoint', -1);
    if LoadInteger < 0 then begin
      if i > 10 then begin
